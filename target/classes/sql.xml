<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mvc.dao.oneMapper">


<!--    如果返回的是一个集合 resultType 类型需要写集合中元素的类型 -->
    <select id="getSelect" resultType="com.mvc.bean.one" >
        select * from one where name = #{name} and sex=#{sex}
    </select>

</mapper>